# Autonomous Trading Company - Environment Variables
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/trading_company
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# LLM Provider
LLM_PROVIDER=anthropic
ANTHROPIC_API_KEY=sk-ant-xxx
LLM_MODEL=claude-3-5-sonnet-20241022

# Optional: OpenAI fallback
OPENAI_API_KEY=sk-xxx

# Trading Credentials (read from credentials.json by default)
# Override here if needed:
# TRADIER_TOKEN=xxx
# JUPITER_API_KEY=xxx

# Nebula Futures
NEBULA_WEBHOOK_KEY=Ob48WT8SpiYCyAQPMaJWN4EWJ1D_puBH
FUTURES_RELAY_URL=http://127.0.0.1:8000

# Worker Settings
WORKER_POLL_INTERVAL=20000
WORKER_CLAIM_TIMEOUT=1800000

# Dashboard
PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:3000

# Heartbeat
HEARTBEAT_AUTH_TOKEN=your-secret-token

# Logging
LOG_LEVEL=info
LOG_FORMAT=text
